{
    "rewrites": [
        {
            "source": "/(.*\\.(js|css|wasm|png|ico|svg|jpg|jpeg|gif|webp))",
            "destination": "/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)\\.wasm",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/wasm"
                }
            ]
        },
        {
            "source": "/(.*\\.(js|css|wasm|png|ico|svg|jpg|jpeg|gif|webp))",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}