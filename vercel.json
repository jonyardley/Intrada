{
    "routes": [
        {
            "src": "/(.*\\.(js|css|wasm|png|ico|svg|jpg|jpeg|gif|webp))",
            "dest": "/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "**/*.wasm",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/wasm"
                }
            ]
        }
    ]
}