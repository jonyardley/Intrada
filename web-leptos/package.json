{
  "name": "web-leptos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:css && trunk serve",
    "build": "npm run build:css && trunk build --release",
    "watch": "trunk watch",
    "clean": "trunk clean",
    "build:css": "tailwindcss -i ./style/input.css -o ./style/output.css",
    "watch:css": "tailwindcss -i ./style/input.css -o ./style/output.css --watch"
  },
  "dependencies": {
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"
  }
}