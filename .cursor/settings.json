{
    "editor.defaultFormatter": "sweetpad",
    "[swift]": {
        "editor.defaultFormatter": "sweetpad",
        "editor.formatOnSave": true
    },
    "sweetpad.enable": true,
    "sweetpad.swiftui.enable": true,
    "sweetpad.swiftui.iosVersion": "18.0",
    "sweetpad.swiftui.previewEnabled": true,
    "sweetpad.swiftui.livePreview": true,
    "sweetpad.swiftui.autoComplete": true,
    "sweetpad.swiftui.snippets": true,
    "swift.buildPath": "${workspaceFolder}/iOS",
    "swift.packagePath": [
        "${workspaceFolder}/shared/Shared",
        "${workspaceFolder}/shared/generated/swift/SharedTypes"
    ],
    "swift.searchSubfoldersForPackages": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "rust-analyzer.check.command": "check",
    "rust-analyzer.cargo.target": "x86_64-apple-darwin",
    "rust-analyzer.cargo.noDefaultFeatures": false,
    "rust-analyzer.procMacro.enable": false,
    "rust-analyzer.cargo.features": [],
    "rust-analyzer.diagnostics.experimental.enable": false
}