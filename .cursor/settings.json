{
    "editor.defaultFormatter": "sweetpad",
    "[swift]": {
        "editor.defaultFormatter": "sweetpad",
        "editor.formatOnSave": true
    },
    "rust-analyzer.checkOnSave": true,
    "sweetpad.enable": true,
    "sweetpad.swiftui.enable": true,
    "sweetpad.swiftui.iosVersion": "18.0",
    "sweetpad.swiftui.previewEnabled": true,
    "sweetpad.swiftui.livePreview": true,
    "sweetpad.swiftui.autoComplete": true,
    "sweetpad.swiftui.snippets": true,
    "rust-analyzer.procMacro.ignored": {
        "leptos_macro": [
            "server"
        ]
    },
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.check.extraArgs": [
        "--all-targets",
        "--all-features",
        "--",
        "-D",
        "warnings"
    ],
    "swift.buildPath": "${workspaceFolder}/iOS",
    "swift.packagePath": [
        "${workspaceFolder}/shared/Shared",
        "${workspaceFolder}/shared/generated/swift/SharedTypes"
    ],
    "swift.searchSubfoldersForPackages": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true
    },
    "files.associations": {
        "**/web-leptos/**/*.rs": "leptos"
    },
    "[leptos]": {
        "editor.defaultFormatter": "leptosfmt",
        "editor.formatOnSave": true
    }
}